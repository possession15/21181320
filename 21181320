//动态规划：用2,5,7元的面值凑出27块钱，用最少的硬币凑出，需要多少硬币？
#include <iostream>
using namespace std;
int function(int x,int a[]){
    int *f=new int[x+1];
    int n=3;
    f[0]=0;
    for(int i=1;i<x+1;i++){
        f[i]=INT32_MAX;
        for(int j=0;j<n;j++){
            if(i>=a[j]&&f[i-a[j]]!=INT32_MAX)
        f[i]=min(f[i-a[j]]+1,f[i]);
        }
    }
    if(f[x]==INT32_MAX){
        return -1;
    }
    else 
    return f[x];
}
int main(){
    int x;
    int a[3];
    cin>>x;
    for(int i=0;i<3;i++){
        cin>>a[i];
    }
    cout<<function(27,a)<<endl;
    system("pause");
    return 0;
}
